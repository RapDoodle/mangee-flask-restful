{
    "DEBUG": true,
    "SECRET_KEY": "THIS_SHOULD_BE_CHANGED",
    "ENABLE_SIMPLE_HTTP_SERVER": true,
    "RESTFUL_PREFIX": "/api",
    "HOST": "0.0.0.0",
    "PORT": 80,
    "LOG_PATH": "./error.log",
    "LOG_FORMAT": "%(asctime)s %(levelname)s: %(message)s",
    "SQLALCHEMY_DATABASE_URI": "sqlite:///data.db",
    "SQLALCHEMY_TRACK_MODIFICATIONS": false,
    "@JWT_AUTH_URL_RULE": "@RESTFUL_PREFIX::/auth",
    "DEFAULT_LANGUAGE": "en-US",
    "@PERMANENT_SESSION_LIFETIME": {
        "type": "timedelta",
        "args": {
            "days": 31
        }
    },
    "JWT_TOKEN_LOCATION": ["cookies"],
    "JWT_COOKIE_SECURE": false,
    "HTTP_SERVER_INDEX_REDIRECT": false,
    "HTTP_SERVER_INDEX_PAGE": "index.html",
    "HTTP_SERVER_404_REDIRECT": "",
    "HTTP_SERVER_REWRITE_ENGINE": true,
    "HTTP_SERVER_REWRITE_TO": "index.html",
    "ENABLE_HTTPS": false,
    "HTTPS_CERT_PATH": "./security/cert.pem",
    "HTTPS_PRIVATE_KEY_PATH": "./security/privkey.pem"
}